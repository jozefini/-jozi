{
  "name": "@jozefini/lib",
  "license": "MIT",
  "version": "0.0.9",
  "description": "JS Library",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "js",
    "library"
  ],
  "author": "Jozefini",
  "repository": {
    "url": "git+https://github.com/jozefini/jozi.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "module": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./events": {
      "import": {
        "types": "./esm/events.d.mts",
        "default": "./esm/events.mjs"
      },
      "module": {
        "types": "./esm/events.d.ts",
        "default": "./esm/events.js"
      },
      "default": {
        "types": "./events.d.ts",
        "default": "./events.js"
      }
    },
    "./html": {
      "import": {
        "types": "./esm/html.d.mts",
        "default": "./esm/html.mjs"
      },
      "module": {
        "types": "./esm/html.d.ts",
        "default": "./esm/html.js"
      },
      "default": {
        "types": "./html.d.ts",
        "default": "./html.js"
      }
    },
    "./object": {
      "import": {
        "types": "./esm/object.d.mts",
        "default": "./esm/object.mjs"
      },
      "module": {
        "types": "./esm/object.d.ts",
        "default": "./esm/object.js"
      },
      "default": {
        "types": "./object.d.ts",
        "default": "./object.js"
      }
    },
    "./string": {
      "import": {
        "types": "./esm/string.d.mts",
        "default": "./esm/string.mjs"
      },
      "module": {
        "types": "./esm/string.d.ts",
        "default": "./esm/string.js"
      },
      "default": {
        "types": "./string.d.ts",
        "default": "./string.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./esm/utils.d.mts",
        "default": "./esm/utils.mjs"
      },
      "module": {
        "types": "./esm/utils.d.ts",
        "default": "./esm/utils.js"
      },
      "default": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "release": "pnpm run build && changesets version && changesets publish && npm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/jozefini/jozi/issues"
  },
  "homepage": "https://github.com/jozefini/jozi#readme",
  "dependencies": {
    "prettier": "^2.8.8"
  }
}
