{
  "name": "@jozefini/lib",
  "license": "MIT",
  "version": "0.0.12",
  "description": "JS Library",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "js",
    "library"
  ],
  "author": "Jozefini",
  "repository": {
    "url": "git+https://github.com/jozefini/jozi.git"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      },
      "module": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./events": {
      "import": {
        "types": "./events.d.mts",
        "default": "./events.mjs"
      },
      "module": {
        "types": "./events.d.ts",
        "default": "./events.js"
      },
      "default": {
        "types": "./events.d.ts",
        "default": "./events.js"
      }
    },
    "./html": {
      "import": {
        "types": "./html.d.mts",
        "default": "./html.mjs"
      },
      "module": {
        "types": "./html.d.ts",
        "default": "./html.js"
      },
      "default": {
        "types": "./html.d.ts",
        "default": "./html.js"
      }
    },
    "./object": {
      "import": {
        "types": "./object.d.mts",
        "default": "./object.mjs"
      },
      "module": {
        "types": "./object.d.ts",
        "default": "./object.js"
      },
      "default": {
        "types": "./object.d.ts",
        "default": "./object.js"
      }
    },
    "./string": {
      "import": {
        "types": "./string.d.mts",
        "default": "./string.mjs"
      },
      "module": {
        "types": "./string.d.ts",
        "default": "./string.js"
      },
      "default": {
        "types": "./string.d.ts",
        "default": "./string.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./utils.d.mts",
        "default": "./utils.mjs"
      },
      "module": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      },
      "default": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "release": "pnpm run build && pnpm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/jozefini/jozi/issues"
  },
  "homepage": "https://github.com/jozefini/jozi#readme",
  "dependencies": {
    "prettier": "^2.8.8"
  }
}
