{
  "name": "@jozefini/lib",
  "license": "MIT",
  "version": "0.2.6",
  "description": "JS Library",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "js",
    "library"
  ],
  "author": "Jozefini",
  "repository": {
    "url": "git+https://github.com/jozefini/jozi.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "module": "./dist/index.js"
    },
    "./events": {
      "import": "./dist/events/index.mjs",
      "module": "./dist/events/index.js"
    },
    "./html": {
      "import": "./dist/html/index.mjs",
      "module": "./dist/html/index.js"
    },
    "./object": {
      "import": "./dist/object/index.mjs",
      "module": "./dist/object/index.js"
    },
    "./string": {
      "import": "./dist/string/index.mjs",
      "module": "./dist/string/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "module": "./dist/utils/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "publish": "pnpm publish",
    "release": "pnpm run build && pnpm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/jozefini/jozi/issues"
  },
  "homepage": "https://github.com/jozefini/jozi#readme"
}
