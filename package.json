{
  "name": "@jozefini/lib",
  "license": "MIT",
  "version": "0.0.12",
  "description": "JS Library",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "js",
    "library"
  ],
  "author": "Jozefini",
  "repository": {
    "url": "git+https://github.com/jozefini/jozi.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./events": {
      "import": {
        "types": "./dist/events.d.mts",
        "default": "./dist/events.mjs"
      },
      "module": {
        "types": "./dist/events.d.ts",
        "default": "./dist/events.js"
      },
      "default": {
        "types": "./dist/events.d.ts",
        "default": "./dist/events.js"
      }
    },
    "./html": {
      "import": {
        "types": "./dist/html.d.mts",
        "default": "./dist/html.mjs"
      },
      "module": {
        "types": "./dist/html.d.ts",
        "default": "./dist/html.js"
      },
      "default": {
        "types": "./dist/html.d.ts",
        "default": "./dist/html.js"
      }
    },
    "./object": {
      "import": {
        "types": "./dist/object.d.mts",
        "default": "./dist/object.mjs"
      },
      "module": {
        "types": "./dist/object.d.ts",
        "default": "./dist/object.js"
      },
      "default": {
        "types": "./dist/object.d.ts",
        "default": "./dist/object.js"
      }
    },
    "./string": {
      "import": {
        "types": "./dist/string.d.mts",
        "default": "./dist/string.mjs"
      },
      "module": {
        "types": "./dist/string.d.ts",
        "default": "./dist/string.js"
      },
      "default": {
        "types": "./dist/string.d.ts",
        "default": "./dist/string.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.mts",
        "default": "./dist/utils.mjs"
      },
      "module": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "default": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "tsc",
    "release": "pnpm run build && pnpm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "bugs": {
    "url": "https://github.com/jozefini/jozi/issues"
  },
  "homepage": "https://github.com/jozefini/jozi#readme",
  "dependencies": {
    "prettier": "^2.8.8"
  }
}
